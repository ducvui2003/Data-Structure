
public class DecimalToBinary {
	public static String decimalToBinaryString(int number) {
		String remainder;
		String quotient;
		if (number /2 ==0) {
			return "1";
		}else {
			remainder = number %2+"";
			quotient = number /2+"";
			return 
		}
	}

	public static void main(String[] args) {
		System.out.println(decimalToBinaryString(25));
	}
}
